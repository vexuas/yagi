{
  "name": "yagi",
  "version": "3.1.0",
  "description": "Vulture's Vale/Blizzard Berg World Boss Timer for Aura Kingdom",
  "main": "yagi.js",
  "author": "Vexuas",
  "scripts": {
    "start": "yarn prebuild && yarn build && yarn build:watch",
    "start:prod": "yarn prebuild && yarn build && BOT_ENV=prod yarn node dist/yagi.js",
    "prebuild": "node -p \"'export const BOT_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
    "build": "tsc",
    "build:watch": "yarn tsc-watch --onSuccess 'nodemon dist/yagi.js'",
    "test": "jest",
    "auto:changelog": "yarn auto changelog",
    "auto:version": "yarn version --`auto version`",
    "auto:release": "yarn auto:changelog && yarn auto:version",
    "release": "git fetch --tags --force && git checkout -b release/$(date +'%Y-%m-%d') && yarn auto:release && git push -u origin head"
  },
  "dependencies": {
    "@discordjs/rest": "^1.0.1",
    "@top-gg/sdk": "^3.1.2",
    "date-fns": "^2.29.3",
    "discord-api-types": "^0.37.0",
    "discord.js": "^13.3.1",
    "googleapis": "39",
    "lodash": "^4.17.21",
    "mixpanel": "^0.13.0",
    "nodemon": "^2.0.7",
    "pg": "^8.9.0",
    "topgg-autoposter": "^2.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/pg": "^8.6.6",
    "auto": "^10.13.1",
    "eslint": "^8.35.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.5.0",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.5"
  }
}
